<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.physicaltests">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!--登录-->
        <activity android:name=".LoginActivity" />
        <!--体型测试-->
        <activity android:name="com.example.test.activity.ShapeActivity"></activity>
        <!--俯卧撑测试-->
        <activity android:name="com.example.test.activity.PushUpActivity"></activity>
        <!--仰卧体坐测试-->
        <activity android:name="com.example.test.activity.SitUpActivity"></activity>
        <!--10*5往返跑测试-->
        <activity android:name="com.example.test.activity.ShuttleRunActivity"></activity>
        <!--跑步测试-->
        <activity android:name="com.example.test.activity.RunningActivity"></activity>

    </application>

</manifest>